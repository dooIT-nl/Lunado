<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.ui.view" id="package_table_tree">
        <field name="name">package.table.tree</field>
        <field name="model">dm.package</field>
        <field name="arch" type="xml">
            <tree create="true" edit="true" export_xlsx="false" delete="true">
                <field name="type" />
                <field name="description" />
                <field name="height" />
                <field name="width" />
                <field name="length" />
                <field name="weight" />
                <field name="is_fragile_package" />
            </tree>
        </field>
    </record>
</odoo>