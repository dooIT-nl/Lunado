<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.ui.view" id="delivery_options_tree">
        <field name="name">delivery.options.tree</field>
        <field name="model">dm.deliver.options</field>
        <field name="arch" type="xml">
            <tree create="false" edit="false" export_xlsx="false" delete="false">
                <header>
                    <button name="setSelectedDeliveryOption" string="Confirm shipping option"
                        type="object" />
                </header>
                <field name="carrierName" />
                <field name="serviceLevelName" />
                <field name="serviceLevelDescription" />
                <field name="deliveryDate" />
                <field name="dm_pickup_date" />
                <field name="buyPrice" />
                <field name="price" string="Sell price" />
            </tree>
        </field>
    </record>


    <record model="ir.ui.view" id="delivery_options_tree_delivery_level">
        <field name="name">delivery.options.tree.delivery.level</field>
        <field name="model">dm.deliver.options</field>
        <field name="arch" type="xml">
            <tree create="false" edit="false" export_xlsx="false" delete="false">
                <header>
                    <button name="set_shipping_option_delivery_level" string="Confirm shipping option" type="object" />
                </header>
                <field name="carrierName" />
                <field name="serviceLevelName" />
                <field name="serviceLevelDescription" />
                <field name="deliveryDate" />
                <field name="dm_pickup_date" />
                <field name="buyPrice" />
                <field name="price" string="Sell price" />
            </tree>
        </field>
    </record>

</odoo>