<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="product_packaging_tree_view2" model="ir.ui.view">
        <field name="name">product.packaging.tree.view2.inherited</field>
        <field name="model">product.packaging</field>
        <field name="inherit_id" ref="product.product_packaging_tree_view2"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='qty']" position="before">
                <field name="min_volume"/>
                <field name="max_volume"/>
                <field name="min_qty"/>
            </xpath>
        </field>
    </record>
</odoo>