<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.ui.view" id="label_table_tree">
        <field name="name">label.table.tree</field>
        <field name="model">dm.label</field>
        <field name="arch" type="xml">
            <tree>
                <field name="id" invisible='1'/>
                <field name="type" />
                <field name="description" />
                <field name="height" />
                <field name="width" />
                <field name="length" />
                <field name="weight" />
                <field name="label_url" widget="url" text="Label"/>
                <field name="tracking_url" widget="url" text="Tracking"/>
                <field name="barcode" />
            </tree>
        </field>
    </record>
</odoo>